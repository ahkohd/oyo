{
  "$schema": "./schema.json",
  "defs": {
    "darkBg": "#263238",
    "darkBgAlt": "#1e272c",
    "darkBgPanel": "#37474f",
    "darkBlue": "#82aaff",
    "darkCyan": "#89ddff",
    "darkFg": "#eeffff",
    "darkFgMuted": "#546e7a",
    "darkGreen": "#c3e88d",
    "darkOrange": "#ffcb6b",
    "darkPink": "#f78c6c",
    "darkPurple": "#c792ea",
    "darkRed": "#f07178",
    "darkViolet": "#bb80b3",
    "darkYellow": "#ffcb6b",
    "lightBg": "#fafafa",
    "lightBgAlt": "#f5f5f5",
    "lightBgPanel": "#e7e7e8",
    "lightBlue": "#6182b8",
    "lightCyan": "#39adb5",
    "lightFg": "#263238",
    "lightFgMuted": "#90a4ae",
    "lightGreen": "#91b859",
    "lightOrange": "#f4511e",
    "lightPink": "#ec407a",
    "lightPurple": "#7c4dff",
    "lightRed": "#e53935",
    "lightViolet": "#945eb8",
    "lightYellow": "#ffb300"
  },
  "theme": {
    "accent": {
      "dark": "darkCyan",
      "light": "lightCyan"
    },
    "background": {
      "dark": "darkBg",
      "light": "lightBg"
    },
    "backgroundElement": {
      "dark": "darkBgPanel",
      "light": "lightBgPanel"
    },
    "backgroundPanel": {
      "dark": "darkBgAlt",
      "light": "lightBgAlt"
    },
    "border": {
      "dark": "#37474f",
      "light": "#e0e0e0"
    },
    "borderActive": {
      "dark": "darkBlue",
      "light": "lightBlue"
    },
    "borderSubtle": {
      "dark": "#1e272c",
      "light": "#eeeeee"
    },
    "diffAdded": {
      "dark": "darkGreen",
      "light": "lightGreen"
    },
    "diffContext": {
      "dark": "darkFgMuted",
      "light": "lightFgMuted"
    },
    "diffLineNumber": {
      "dark": "#37474f",
      "light": "#cfd8dc"
    },
    "diffRemoved": {
      "dark": "darkRed",
      "light": "lightRed"
    },
    "error": {
      "dark": "darkRed",
      "light": "lightRed"
    },
    "info": {
      "dark": "darkOrange",
      "light": "lightOrange"
    },
    "primary": {
      "dark": "darkBlue",
      "light": "lightBlue"
    },
    "secondary": {
      "dark": "darkPurple",
      "light": "lightPurple"
    },
    "success": {
      "dark": "darkGreen",
      "light": "lightGreen"
    },
    "text": {
      "dark": "darkFg",
      "light": "lightFg"
    },
    "textMuted": {
      "dark": "darkFgMuted",
      "light": "lightFgMuted"
    },
    "warning": {
      "dark": "darkYellow",
      "light": "lightYellow"
    }
  }
}
