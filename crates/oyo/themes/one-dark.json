{
  "$schema": "./schema.json",
  "defs": {
    "darkBg": "#282c34",
    "darkBgAlt": "#21252b",
    "darkBgPanel": "#353b45",
    "darkBlue": "#61afef",
    "darkCyan": "#56b6c2",
    "darkFg": "#abb2bf",
    "darkFgMuted": "#5c6370",
    "darkGreen": "#98c379",
    "darkOrange": "#d19a66",
    "darkPurple": "#c678dd",
    "darkRed": "#e06c75",
    "darkYellow": "#e5c07b",
    "lightBg": "#fafafa",
    "lightBgAlt": "#f0f0f1",
    "lightBgPanel": "#eaeaeb",
    "lightBlue": "#4078f2",
    "lightCyan": "#0184bc",
    "lightFg": "#383a42",
    "lightFgMuted": "#a0a1a7",
    "lightGreen": "#50a14f",
    "lightOrange": "#986801",
    "lightPurple": "#a626a4",
    "lightRed": "#e45649",
    "lightYellow": "#c18401"
  },
  "theme": {
    "accent": {
      "dark": "darkCyan",
      "light": "lightCyan"
    },
    "background": {
      "dark": "darkBg",
      "light": "lightBg"
    },
    "backgroundElement": {
      "dark": "darkBgPanel",
      "light": "lightBgPanel"
    },
    "backgroundPanel": {
      "dark": "darkBgAlt",
      "light": "lightBgAlt"
    },
    "border": {
      "dark": "#393f4a",
      "light": "#d1d1d2"
    },
    "borderActive": {
      "dark": "darkBlue",
      "light": "lightBlue"
    },
    "borderSubtle": {
      "dark": "#2c313a",
      "light": "#e0e0e1"
    },
    "diffAdded": {
      "dark": "darkGreen",
      "light": "lightGreen"
    },
    "diffContext": {
      "dark": "darkFgMuted",
      "light": "lightFgMuted"
    },
    "diffLineNumber": {
      "dark": "#495162",
      "light": "#c9c9ca"
    },
    "diffRemoved": {
      "dark": "darkRed",
      "light": "lightRed"
    },
    "error": {
      "dark": "darkRed",
      "light": "lightRed"
    },
    "info": {
      "dark": "darkOrange",
      "light": "lightOrange"
    },
    "primary": {
      "dark": "darkBlue",
      "light": "lightBlue"
    },
    "secondary": {
      "dark": "darkPurple",
      "light": "lightPurple"
    },
    "success": {
      "dark": "darkGreen",
      "light": "lightGreen"
    },
    "text": {
      "dark": "darkFg",
      "light": "lightFg"
    },
    "textMuted": {
      "dark": "darkFgMuted",
      "light": "lightFgMuted"
    },
    "warning": {
      "dark": "darkYellow",
      "light": "lightYellow"
    }
  }
}
